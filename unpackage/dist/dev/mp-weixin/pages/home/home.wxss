.home {
  /* 轮播图 */
  /* 子导航 */
  float: left;
}
.home swiper {
  width: 100vw;
  height: 340rpx;
}
.home swiper swiper-item image {
  width: 100%;
  height: 340rpx;
}
.home .sub-nav {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
}
.home .sub-nav .nav-list {
  width: 25%;
  text-align: center;
}
.home .sub-nav .nav-list image {
  width: 128rpx;
  height: 140rpx;
}
.home .floor {
  overflow: hidden;
}
.home .floor .title {
  margin: 10rpx 0;
}
.home .floor .title image {
  width: 100vw;
  height: 88rpx;
}
.home .floor .content {
  float: left;
}
.home .floor .content image {
  width: 232rpx;
  height: 386rpx;
}
.home .floor .content:nth-child(2) {
  margin-right: 10rpx;
}
.home .floor .content:nth-child(n+3) {
  width: 232rpx;
  height: 188rpx;
  margin-right: 10rpx;
}
.home .floor .content:nth-child(n+3) image {
  width: 100%;
  height: 100%;
  object-fit: cover;
}
.home .floor .content:nth-last-child(-n+2) {
  margin-top: 10rpx;
}

