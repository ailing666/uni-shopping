.category .list {
  width: 200rpx;
  background-color: #f5f5f5;
}
.category .list view {
  text-align: center;
  height: 120rpx;
}
.category .list view text {
  display: inline-block;
  width: 200rpx;
  height: 100rpx;
  padding: 30rpx;
  box-sizing: border-box;
}
.category .list .active {
  background-color: #FFFFFF;
  color: #ff2d4a;
  font-weight: 700;
  border-left: 6rpx solid #ff3954;
}

